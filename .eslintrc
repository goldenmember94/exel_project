{
    "parser": "babel-eslint", 
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "eol-last":"off",
        "require-jsdoc": "off",
        "object-curly-spacing":"off",
        "quotes":"off",
        "comma-dangle":"off",
        "spaced-comment":"off",
        "operator-linebreak":"off"
    },
    "extends": ["eslint:recommended", "google"]
}